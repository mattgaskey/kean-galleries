<div class="scroll-nav flex-flex-col space-y-7">
  <ul class="scroll-nav--list list-none text-lg">
    {% for item in items %}
      <li class="relative pl-6 py-4 flex items-center before:content-empty before:block before:absolute before:left-0 before:w-3-px before:h-full before:bg-yellow">{{ item }}</li>
    {% endfor %}
  </ul>
